   "source": [
    "from sklearn import metrics\n",
    "import catboost as cb\n",
    "\n",
    "class CustomLoss:\n",
    "    def is_max_optimal(self):\n",
    "        return True # greater is better\n",
    "    \n",
    "    def calc_ders_range(self, approxes, targets, weights):\n",
    "        # Calculate gradients of the custom loss function\n",
    "        grad = -0.1 * np.mean(np.sum(gain * approxes[0], axis=1))\n",
    "        # Calculate Hessians of the custom loss function (not used in this example)\n",
    "        hess = np.zeros_like(grad)\n",
    "        return grad, hess\n",
    "\n",
    "def custom_metric(y_true, y_pred):\n",
    "    # Implement your custom evaluation metric\n",
    "    # For example, you can calculate Mean Absolute Error (MAE)\n",
    "    return np.mean(np.abs(y_true - y_pred))"
   ]
